{
    "name": "systemin/sylius-google-merchant-center",
    "description": "A Sylius plugin to integrate with Google Merchant Center.",
    "type": "sylius-plugin",
    "keywords": [
        "sylius",
        "sylius-plugin",
        "google merchant center",
        "product feed"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Lilian",
            "email": "lilian@systemin.fr"
        }
    ],
    "require": {
        "php": "^8.1",
        "sylius/sylius": "~1.13.0",
        "symfony/webpack-encore-bundle": "^1.15"
    },
    "require-dev": {
        "php": "^8.1",
        "sylius/sylius": "~1.13.0",
        "symfony/webpack-encore-bundle": "^1.15"
    },
    "conflict": {
        "symfony/framework-bundle": "6.2.8"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "phpstan/extension-installer": true,
            "symfony/flex": true
        }
    },
    "extra": {
        "symfony": {
            "require": "^5.4 || ^6.4"
        }
    },
    "autoload": {
        "psr-4": {
            "Systemin\\SyliusGoogleMerchantCenter\\": "src/"
        }
    }
}
